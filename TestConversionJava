# Java


public class TestConversion {
	
	public static void main(String[] args){
		double salario = 1270.50;
		int valor = (int) salario ;
		
		System.out.println(valor);
		
		
		//prueba
		 double salario1 = 1270.50;
	        int valors = (int) salario1;
	        System.out.println(valor);

	        double valors1 = 0.2;
	        double valor2 = 0.1;
	        double total = valors1 + valor2;

	        System.out.println(total);
	        
	       
	        //otra manera
	        
	        float puntoFlotante = 3.14F;   // aqui!

	        double salario2 = 1270.50;
	        int valores = (int) salario2;
	        System.out.println(valores);

	        double valor1 = 0.2;
	        double valors2 = 0.1;
	        double totals = valor1 + valors2;

	        System.out.println(totals);
	        		
	}

}

